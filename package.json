{
  "name": "giftpickr",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "add-page": "ng g m pages/page-name --routing && ng g component pages/page-name --skip-tests",
    "dev:ssr": "ng run giftpickr:serve-ssr",
    "serve:ssr": "node dist/giftpickr/server/main.js",
    "build:ssr": "ng build && ng run giftpickr:server",
    "prerender": "ng run giftpickr:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.2",
    "@angular/cdk": "^15.2.2",
    "@angular/cli": "^15.2.2",
    "@angular/common": "^15.2.2",
    "@angular/compiler": "^15.2.2",
    "@angular/core": "^15.2.2",
    "@angular/forms": "^15.2.2",
    "@angular/localize": "^15.2.2",
    "@angular/material": "^15.2.2",
    "@angular/platform-browser": "^15.2.2",
    "@angular/platform-browser-dynamic": "^15.2.2",
    "@angular/platform-server": "^15.0.0",
    "@angular/router": "^15.2.2",
    "@nguniversal/express-engine": "^15.2.0",
    "angular-svg-icon": "15.0.0",
    "axios": "^1.3.3",
    "bootstrap": "^5.2.3",
    "express": "^4.15.2",
    "material-icons": "^1.13.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.5",
    "@angular/compiler-cli": "^15.1.4",
    "@nguniversal/builders": "^15.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.13",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  }
}
